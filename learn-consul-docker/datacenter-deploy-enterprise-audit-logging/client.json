{
    "node_name": "consul-client",
    "data_dir": "/consul/data",
    "retry_join":[
        "consul-server1",
        "consul-server2",
        "consul-server3"
     ],
    "acl": {
      "enabled": true,
      "default_policy": "allow",
      "down_policy": "extend-cache",
      "enable_token_persistence" : true
    },
    "audit": {
        "enabled": true,
        "sink": {
            "My sink": {
                "type": "file",
                "format": "json",
                "path": "/consul/data/audit.json",
                "delivery_guarantee": "best-effort",
                "rotate_duration": "24h",
                "rotate_max_files": 15,
                "rotate_bytes": 25165824
            }
        }
    }
}